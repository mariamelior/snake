{
  "lintOptions": {
    "typeCheck": true
  },
  "defaultSeverity": "error",
  "rules": {
    "max-line-length": [true, 125],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "ordered-imports": [false],
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": [true, 2]
  }
}
